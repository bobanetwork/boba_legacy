{
   "name": "@boba/teleportation",
   "version": "1.0.0",
   "description": "The teleportation between L2s",
   "main": "dist/src/index",
   "files": [
      "dist/src/index",
      "test/**/*.ts"
   ],
   "scripts": {
      "start": "ts-node src/index.ts",
      "build": "tsc -p ./tsconfig.json",
      "clean": "rimraf  dist/ ./tsconfig.tsbuildinfo",
      "lint": "yarn lint:fix && yarn lint:check",
      "lint:fix": "prettier --config .prettierrc.json --write \"{src,exec,test}/**/*.ts\"",
      "lint:check": "tslint --format stylish --project .",
      "db:start": "docker run -e POSTGRES_DB=teleportation -e POSTGRES_PASSWORD=abcdef -p 5432:5432 postgres",
      "test": "hardhat test --show-stack-traces",
      "test:coverage": "nyc hardhat test && nyc merge .nyc_output coverage.json",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "license": "MIT",
   "devDependencies": {
      "@boba/contracts": "0.0.1",
      "@eth-optimism/core-utils": "0.8.1",
      "@nomiclabs/hardhat-ethers": "^2.0.2",
      "@nomiclabs/hardhat-waffle": "^2.0.1",
      "@types/mocha": "^8.2.2",
      "@types/node": "^16.11.10",
      "chai": "^4.3.4",
      "ethereum-waffle": "^3.3.0",
      "ethers": "^5.5.4",
      "hardhat": "^2.12.5",
      "mocha": "^8.3.1",
      "ts-node": "10.7.0",
      "tslint": "^6.1.3",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@eth-optimism/common-ts": "0.2.2",
      "@eth-optimism/contracts": "0.5.11",
      "bcfg": "^0.1.6",
      "dotenv": "^8.6.0",
      "ethers": "^5.5.4",
      "lodash": "^4.17.21",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.16"
   }
}
